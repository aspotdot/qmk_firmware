        "mousekey": true,
        "nkro": true,
        "encoder": true,
        "extrakey": true,

        "bootmagic": false,
        "console":false,
        "command": false,
        "rgblight": false,
        "backlight": false,
        "quantum_painter": false
    },

    "encoder": {
            "rotary": [
                { "pin_a": "D19", "pin_b": "D20", "resolution": 4 },
            ]
        },

